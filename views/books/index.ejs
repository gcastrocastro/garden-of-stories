<!DOCTYPE html>
<html>
<%- include('../partials/head') %>
<body>
<%- include('../partials/header') %>
<div class="book_container">
    <% books.forEach((b) => { %>
        <div class="individual_book">
            <a href="/books/<%= b._id %>">
                <img src="<%= b.imageUrl%>" alt="<%=b.title%>" />
                <p><%= b.title %></p>
            </a>
        </div>
    <% }) %>
</div>
<%- include('../partials/footer') %>
</body>
</html>
